//String Methods:
//methods of string are not change the original value, it return the new values

//toUpperCase
let str="ApnaCollege";
console.log(str.toUpperCase());         //Print in upprcase

//toLowerCase
console.log(str);
console.log(str.toLowerCase());         ////Print in lowercase

//trim                                      //It trimp start and end space only
let str2="    Apana    College    JS    ";
console.log(str2.trim());

//Slice
let str3="01234567";
console.log(str3.slice(1,5));           //1234

//String Concating: Join two strings:
//we can concat string in three type-

let str4="Apna";
let str5="College";
let res=str4.concat(str5);
console.log(res);
//console.log(str4.concat(str5));
